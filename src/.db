CREATE TABLE carros (
    `marca` VARCHAR(50) NOT NULL ,
     `modelo` VARCHAR(100) NOT NULL ,
      `descrição` TEXT NULL ,
       `preco` DECIMAL(10 , 2) NOT NULL ,
        `contato` VARCHAR(20) NOT NULL ,
        `ft1` LONGTEXT NOT NULL ,
         `ft2` LONGTEXT NULL ,
          `ft3` LONGTEXT NULL ,
           `ft4` LONGTEXT NULL ,
            `ft5` LONGTEXT NULL ,
             `id` INT NOT NULL AUTO_INCREMENT , PRIMARY KEY (`id`)) 
             ;